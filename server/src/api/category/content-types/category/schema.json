{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "category"
    },
    "sub_categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sub-category.sub-category",
      "mappedBy": "category"
    },
    "code": {
      "required": true,
      "max": 999,
      "type": "integer",
      "min": 100
    }
  }
}
